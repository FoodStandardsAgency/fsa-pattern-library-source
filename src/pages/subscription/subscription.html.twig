{% block tabs %}
  {% set tabs = [
    {
      is_active: false,
      name: 'Food and allergy',
      link: '#',
    },
    {
      is_active: false,
      name: 'News and consultations',
      link: '#',
    },
    {
      is_active: true,
      name: 'Delivery options',
      link: '#',
    },
    {
      is_active: false,
      name: 'Account settings',
      link: '#',
    },
  ] %}
  {% include '@components/components/form/Tabs/tabs.html.twig' with {
    type: 'Subscription details',
    tabs: tabs,
  } %}
{% endblock %}
