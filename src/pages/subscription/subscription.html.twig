    {
      isActive: false,
      name: 'News and consultations',
      link: '#',
    },
    {
      isActive: true,
      name: 'Delivery options',
      link: '#',
    },
    {
      isActive: false,
      name: 'Account settings',
      link: '#',
    },
  ] %}
  {% include '../../components/form/Tabs/tabs.html.twig' with {
    type: 'Subscription details',
    tabs: tabs,
  } %}
{% endblock %}
