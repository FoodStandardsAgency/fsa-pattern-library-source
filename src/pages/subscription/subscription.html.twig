{% extends "@components/layout/formLayout/formLayout.html.twig" %}
{% block top %}
  {# Include templates for form components here #}
  {% set breadcrumb_items = [
    {
      'text': 'Home',
      'url': '/'
    },
    {
      'text': 'News & Alerts',
      'url': '/news-alerts'
    },
    {
      'text': 'Subscribe to news & alerts',
      'url': null
    },
  ] %}
  {% include '@components/components/general/Breadcrumb/breadcrumbs.html.twig' with {
    items: breadcrumb_items,
    expanded: false,
    subscription: {
      is_logged_in: false,
      logged_out_label: 'Already have a subscription?',
      logged_out_button_label: 'Sign in',
      logged_out_button_link: '#',
    }
  } %}
