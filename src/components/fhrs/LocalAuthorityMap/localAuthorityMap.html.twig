    <div class="local-authority-map__table-wrapper">
      <div class="local-authority-map__table text-block">
        {% if selected_region %}
          <h2>{{ selected_region_title_prefix }} {{ selected_region|replace({'-': ' '})|title}} {{ selected_region_title_suffix }}</h2>
          <table>
            <tr>
              <th>{{ local_authority_label }}</th>
              <th>{{ number_of_businesses_label }}</th>
            </tr>
            {% for data in selected_results_data %}
              <tr>
                <td><a href="{{ data.local_authority_link }}">{{ data.local_authority }}</a></td>
                <td>{{ data.number_of_businesses }}</td>
              </tr>
            {% endfor %}
          </table>
        {% else %}
          <h2>{{ default_map_title }}</h2>
          <table>
            {% for region in regions %}
              <tr>
                <td><a href="{{ region.link }}">{{ region.name }}</a></td>
              </tr>
            {% endfor %}
          </table>
        {% endif %}
      </div>
      {% if selected_region %}
      <div class="local-authority-map__pagination">
        {% include '@components/components/search/Pagination/pagination.html.twig' with {
          total_pages: page_size,
          current_page: page,
          max_visible_pages: 4,
          first_label: "First",
          previous_label: "Previous",
          next_label: "Next",
          last_label: "Last",
        } %}
      </div>
      {% endif %}
