<div class="foot-notes">
    <ol class="foot-notes__link-list">
      {% for link in links %}
        <li class="foot-notes__link-list__item">
          <span class="foot-notes__link-list__item-label-prefix">{{ link.label_prefix }}</span>
          <a href="#fnref:{{ link.key }}" class="foot-notes__link-list__item-link">{{ link.label }}</a>
          <span class="foot-notes__link-list__item-label-postfix">{{ link.label_postfix }}</span>
        </li>
      {% endfor %}
    </ol>
</div>
