<table class="webtool-studies-table">
  <thead>
    <tr>
      <th>{{ study_name }}</th>
      <th>{{ created_on }}</th>
      <th>{{ last_updated }}</th>
      <th colspan="3" scope="colgroup">{{ status }}</th>
    </tr>
  </thead>
  <tbody>
  {% for row in rows %}
    <tr>
      <td>{{ row.label }}</td>
      <td>{{ row.created_at }}</td>
      <td>{{ row.updated_at }}</td>
      <td>{{ row.status }}</td>
      <td class="right">
        {% include '@components/components/general/Button/button.html.twig' with {
          element: 'link',
          type: 'primary',
          url: row.button.url,
          label: row.button.label,
        } %}
      </td>
      <td>
        {% include '@components/components/general/DropdownMenu/dropdownMenu.html.twig' with {
          label: 'More options',
          options: row.options,
        } %}
      </td>
    </tr>
  {% endfor %}
  </tbody>
</table>